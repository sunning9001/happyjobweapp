.header{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 88rpx;
  padding: 0 20rpx;
  border-bottom: 2rpx solid #eaeaea;
  font-size: 28rpx;
}
.cancel{
  flex: 0 0 auto;
  padding-left: 40rpx;
  padding-right: 20rpx;
  color: #333 ;
}
.header .search{
  display: flex;
  flex-grow: 1;
  align-items: center;
  height: 70rpx;
  padding: 0 20rpx;
  background:#f5f5f5;
  border-radius: 35rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #999;
}
.search-icon{
  width: 30rpx;
  height: 30rpx;
}
.search-input{
  flex-grow: 1;
  margin-left: 10rpx;
  color: #333;
}
.bot-dist{
  border-bottom: 20rpx solid #EDEBEB;
}
.topic{
  padding: 0 30rpx;
  color: #999;
  font-size: 28rpx;
}
.topic .topic-header{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
}
.trash{
  width: 32rpx;
}
.record{
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  padding: 0 10rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #7D7D7D;
  border: 2rpx solid #999;
  border-radius: 6rpx;
}
/* 搜索结果列表 */
.search-list .li{
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 30rpx;
  color: #333;
  font-size: 28rpx;
  border-bottom: 2rpx solid #eaeaea;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
